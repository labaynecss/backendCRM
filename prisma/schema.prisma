  deleted Int      @map("_DELETED")
  date    DateTime @default(now(), map: "DF__branch___DATE__4D2A7347") @map("_DATE") @db.DateTime

  @@index([ID], map: "ID")
}

model areas {
  ID            Int     @id(map: "PK_areas_ID") @default(autoincrement())
