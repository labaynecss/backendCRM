  userIdNumber String  @default("N'0'", map: "DF__users__USER_ID_N__36F11965") @map("USER_ID_NUMBER") @db.NVarChar(60)

  @@index([ID], map: "ID")
  @@index([ID], map: "ID_2")
}

model branch {
  ID      Int      @id(map: "PK_branch_ID") @default(autoincrement())
  branch  String   @map("BRANCH") @db.NVarChar(45)
