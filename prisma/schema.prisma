generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
  theme    = "dark"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model users {
  ID           Int     @id(map: "PK_users_ID") @default(autoincrement())
  USERNAME     String? @db.NVarChar(30)
  password     String? @map("_PASSWORD") @db.NVarChar(64)
