model crm_collection_area {
  id            Int    @id(map: "PK_crm_collection_area_id") @default(autoincrement())
  zond_code     String @map("ZOND_CODE") @db.VarChar(255)
  zond_areacode String @map("ZOND_AREACODE") @db.VarChar(255)
  zond_areaname String @map("ZOND_AREANAME") @db.NVarChar(255)
  status        Int    @default(1, map: "DF__crm_colle__STATU__314D4EA8") @map("STATUS")
  deleted       Int    @default(0, map: "DF__crm_colle___DELE__324172E1") @map("_DELETED")

  @@index([id], map: "id")
  @@index([id], map: "id_2")
  @@index([id], map: "id_3")
}

model crm_business_type {
  ID       Int     @id(map: "PK_crm_business_type_ID") @default(autoincrement())
  refecode String? @map("REFE_CODE") @db.NVarChar(255)
  refname  String? @map("REFE_NAME") @db.NVarChar(255)
  delete   Int     @map("_DELETED")

  @@index([ID], map: "ID")
  @@index([ID], map: "ID_2")
}

model crm_product {
  pid          Int     @id(map: "PK_crm_product_pid") @default(autoincrement()) @map("pid")
  product_name String? @map("product_name") @db.VarChar(255)
  product_type String? @map("product_type") @db.VarChar(255)
  cam          Int     @default(0, map: "DF__crm_product__cam__1C1D2798") @map("cam")
  active       String? @map("active") @db.VarChar(255)
  company      String? @map("company") @db.VarChar(255)

  @@index([pid], map: "pid")
  @@index([pid], map: "pid_2")
}



